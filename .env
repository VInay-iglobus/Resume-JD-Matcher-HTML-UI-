# Resume-JD Matcher Configuration
# Copy this to .env file and fill in your values

# JWT Secret Key (CRITICAL - must be consistent)
# Change this to a random string for production
SECRET_KEY=resume-matcher-secret-key-2024-fixed

# Database Configuration (optional)
# DATABASE_URL=sqlite:///./data.db

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2:latest

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# CORS Configuration
CORS_ORIGINS=*

# Logging
LOG_LEVEL=INFO